{
  "config": {
    "sort-packages": true
  },
  "name": "pe/component-simple-di",
  "description": "Simple dependency injection container component",
  "keywords": ["dependency injection", "psr-11"],
  "authors": [
    {
      "name": "Pavlenko Eugeniy",
      "email": "pavlenko.obs@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "suggest": {},
  "provide": {
    "psr/container-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "PE\\Component\\SimpleDI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PETest\\Component\\SimpleDI\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}